						"karmaConfig": "projects/collection/karma.conf.js",
						"assets": [
							"projects/collection/src/favicon.ico",
							"projects/collection/src/assets"
						],
						"styles": [
							"projects/collection/src/styles.css"
						],
						"scripts": []
					}
				}
			}
		},
